#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n,i;
    scanf("%d", &n);
    int ar[1000];
    for(i=0;i<n;i++)
    {
        scanf("%d", &ar[i]);
        
    }
    for(i=n-1;i>=0;i--)
    {
        printf("%d\t",ar[i]);
    }
    return 0;
}
